-edit-profile-routing.module';\n\nimport { AssoEditProfilePage } from './asso-edit-profile.page';\n\n@NgModule({\n  imports: [\n    CommonModule,\n    FormsModule,\n    IonicModule,\n    AssoEditProfilePageRoutingModule\n  ],\n  declarations: [AssoEditProfilePage]\n})\nexport class AssoEditProfilePageModule {}\n","export default \"\\n/*# sourceMappingURL=data:application/json;base64,eyJ2ZXJzaW9uIjozLCJzb3VyY2VzIjpbXSwibmFtZXMiOltdLCJtYXBwaW5ncyI6IiIsImZpbGUiOiJzcmMvYXBwL2Fzc28tZWRpdC1wcm9maWxlL2Fzc28tZWRpdC1wcm9maWxlLnBhZ2Uuc2NzcyJ9 */\";","import { AngularFireStorage } from '@angular/fire/storage';\nimport { LoadingController } from '@ionic/angular';\nimport { Camera, CameraOptions } from '@ionic-native/camera/ngx';\nimport { Component, OnInit } from '@angular/core';\nimport { AngularFireAuth } from '@angular/fire/auth';\nimport { AngularFirestore, AngularFirestoreDocument, AngularFirestoreCollection } from '@angular/fire/firestore';\nimport { ActivatedRoute, Router, NavigationExtras } from '@angular/router';\n\n@Component({\n  selector: 'app-asso-edit-profile',\n  templateUrl: './asso-edit-profile.page.html',\n  styleUrls: ['./asso-edit-profile.page.scss'],\n})\nexport class AssoEditProfilePage implements OnInit {\n  userId = '';\n  loading: any;\n  imagePath: string;\n  imageUrl: string;\n  upload: any;\n\n  dataUser = {\n    logocrop: '',\n    nom: '',\n    description: 'Ajouter une description',\n    tel: 'Ajouter un numéro de téléphone',\n    email: '',\n    adresse: 'Ajouter une adresse',\n    siret: 'Ajouter un numéro de siret',\n    logo:''\n  };\n  \n\n  constructor(private route: ActivatedRoute,private router: Router,\n    public afAuth: AngularFireAuth,\n    private firestore: AngularFirestore,\n    private camera: Camera,\n    public loadingController: LoadingController,\n    public afSG: AngularFireStorage,) { \n\n  }\n\n  ngOnInit() {\n    //recep dataUser\n    this.route.queryParams.subscribe(params =>{\n      console.log('params:', params);\n      if (this.router.getCurrentNavigation().extras.state){\n        this.dataUser= this.router.getCurrentNavigation().extras.state.dataUser;\n        console.log(\"recep\" + this.dataUser.nom);\n        \n      }\n    })\n        //recup info auth\n        this.afAuth.authState.subscribe(auth => {\n          this.userId = auth.uid;\n          console.log(\"userID:   \" + this.userId)\n    \n        });\n   \n  }\n\n  async uploadFirebase() {\n    this.presentLoading('Enregistrement des modifications');\n    // stocker dans un dossier -->  this.imagePath = 'Users/'+ new Date().getTime() + 'jpg'\n    this.imagePath = 'LogoAssos/' + this.dataUser.nom + this.userId + 'jpg'\n    // Upload image stockée dans image de type data_url\n    this.afSG.ref(this.imagePath).putString(this.dataUser.logocrop, 'data_url');\n    this.upload = this.afSG.ref(this.imagePath).putString(this.dataUser.logocrop, 'data_url');\n    console.log('url' + this.imagePath)\n    // .then --> une fois qu'on a envoyé l'image\n    this.upload.then(async () => {\n      //get download Url\n      this.afSG.ref(this.imagePath).getDownloadURL().subscribe(imgUrl => {\n        console.log('down   ' + imgUrl);\n        this.imageUrl = imgUrl;\n\n        this.uploadFirestore();\n      });\n\n    \n    });\n  }\n\n  uploadFirestore() {\n    this.afAuth.authState.subscribe(auth => {this.userId = auth.uid;\n      console.log('ici' + this.userId)\n      //console.log('click' + this.imageUrl)\n    console.log('click' + this.userId)\n    this.firestore.collection(\"privateassos\").doc(this.userId).set({\n      name: this.dataUser.nom,\n      logo: this.imageUrl,\n      AssoSIRET: this.dataUser.siret,\n      AssoDescription:this.dataUser.description,\n      AssoTel:this.dataUser.tel,\n      AssoAdress:this.dataUser.adresse\n      //logo: this.imageUrl\n    })\n      .then(function () {\n        console.log(\"Document successfully written!\");\n        this.loading.dismiss();\n    this.router.navigate(['/tabs/tab4']);\n      })\n      .catch(function (error) {\n        console.error(\"Error writing document: \", error);\n      });\n    });\n    \n\n  }\n\n   // async car temps de choisir photo non limité\n   async addPhoto(source: string) {  \n      console.log('library');\n      //stocke image selectionné dans libraryImage\n      const libraryImage = await this.openLibrary();\n       // console.log('hello');\n       \n       // \n\n     \n      //base64 rend le fichier visible en html\n     \n  }\n\n   //ionic existing fonction ouvre la librarie\n   async openLibrary() {\n    const options: CameraOptions = {\n      quality: 100,\n      destinationType: this.camera.DestinationType.DATA_URL,\n      encodingType: this.camera.EncodingType.JPEG,\n      mediaType: this.camera.MediaType.PICTURE,\n      targetWidth: 1000,\n      targetHeight: 1000,\n      sourceType: this.camera.PictureSourceType.PHOTOLIBRARY\n    };\n    return await this.camera.getPicture(options).then((ImageData)=>{\n      this.dataUser.logo = 'data:image/jpg;base64,' + ImageData;\n        this.Gocrop();\n    })\n  }\n\n  // go to crop and pass dataUser\n  Gocrop() {\n    console.log('crop');\n    let navigationExtras: NavigationExtras = {\n      state: {\n        dataUser: this.dataUser\n      }\n    }\n    this.router.navigate(['croplogoedit'], navigationExtras);\n  }\n\n  async presentLoading(message: string){\n    this.loading = await this.loadingController.create({message})\n    return this.loading.present();\n  }\n\n  \n\n\n}\n"]}