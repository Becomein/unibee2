        this.userId = auth.uid;\n          console.log(\"userID:   \" + this.userId)\n    \n        });\n   \n  }\n\n  uploadFirestore() {\n    this.afAuth.authState.subscribe(auth => {this.userId = auth.uid;\n      console.log('ici' + this.userId)\n      //console.log('click' + this.imageUrl)\n    console.log('click' + this.userId)\n    this.firestore.collection(\"privateassos\").doc(this.userId).set({\n      name: this.dataUser.nom,\n      logo: this.dataUser.logocrop,\n      AssoSIRET: this.dataUser.siret,\n      AssoDescription:this.dataUser.description,\n      AssoTel:this.dataUser.tel,\n      AssoAdress:this.dataUser.adresse\n      //logo: this.imageUrl\n    })\n      .then(function () {\n        console.log(\"Document successfully written!\");\n      })\n      .catch(function (error) {\n        console.error(\"Error writing document: \", error);\n      });\n    });\n    this.router.navigate(['/tabs/tab4']);\n\n  }\n\n   // async car temps de choisir photo non limité\n   async addPhoto(source: string) {  \n      console.log('library');\n      //stocke image selectionné dans libraryImage\n      const libraryImage = await this.openLibrary().then(success => {\n        console.log('hello');\n        this.dataUser.logo = 'data:image/jpg;base64,' + libraryImage;\n        this.Gocrop();\n\n      }, function (error) {\n        console.log('Email ou mot de passe mal formaté');\n      }\n      );\n      //base64 rend le fichier visible en html\n     \n  }\n\n   //ionic existing fonction ouvre la librarie\n   async openLibrary() {\n    const options: CameraOptions = {\n      quality: 100,\n      destinationType: this.camera.DestinationType.DATA_URL,\n      encodingType: this.camera.EncodingType.JPEG,\n      mediaType: this.camera.MediaType.PICTURE,\n      targetWidth: 1000,\n      targetHeight: 1000,\n      sourceType: this.camera.PictureSourceType.PHOTOLIBRARY\n    };\n    return await this.camera.getPicture(options).then((ImageData) => {});\n  }\n  // go to edit and pass dataUser\n  Gocrop() {\n    console.log('crop');\n    let navigationExtras: NavigationExtras = {\n      state: {\n        dataUser: this.dataUser\n      }\n    }\n    this.router.navigate(['croplogo'], navigationExtras);\n  }\n\n  \n\n\n}\n"],"sourceRoot":"webpack:///"}